#tag WindowCode
	#tag Event
		Sub Open()
		  #if TargetMacOS then
		    btnLeft.Default = false
		    btnLeft.Cancel = true
		    
		    btnRight.Default = true
		    btnRight.Cancel = false
		    
		  #else
		    btnLeft.Default = true
		    btnLeft.Cancel = false
		    
		    btnRight.Default = false
		    btnRight.Cancel = true
		    
		  #endif
		  
		  me.CancelButton.Caption = kCancel
		  me.DefaultButton.Caption = kOK
		  
		  RaiseEvent Open
		End Sub
	#tag EndEvent


	#tag Method, Flags = &h0
		Function CancelButton() As PushButton
		  #if TargetMacOS then
		    return btnLeft
		    
		  #else
		    return btnRight
		    
		  #endif
		End Function
	#tag EndMethod

	#tag Method, Flags = &h0
		Function DefaultButton() As PushButton
		  #if TargetMacOS then
		    return btnRight
		    
		  #else
		    return btnLeft
		    
		  #endif
		End Function
	#tag EndMethod


	#tag Hook, Flags = &h0
		Event ClickedCancel()
	#tag EndHook

	#tag Hook, Flags = &h0
		Event ClickedOK()
	#tag EndHook

	#tag Hook, Flags = &h0
		Event Open()
	#tag EndHook


	#tag Constant, Name = kCancel, Type = String, Dynamic = True, Default = \"Cancel", Scope = Protected
